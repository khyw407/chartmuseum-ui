<!DOCTYPE html>
<html lang="ko">
    <head>
        <meta charset="UTF-8">
        <meta name="viewport" content="width=device-width, initial-scale=1.0">
        <link rel="stylesheet" href="https://fonts.googleapis.com/css?family=Source+Sans+Pro:300,400,400i,700&display=fallback">
            <link rel="stylesheet" href="/lib/external/admin-lte/plugins/fontawesome-free/css/all.min.css">
            <link rel="stylesheet" href="https://code.ionicframework.com/ionicons/2.0.1/css/ionicons.min.css">
            <link rel="stylesheet" href="/lib/external/admin-lte/dist/css/adminlte.min.css">
            <link rel="stylesheet" href="/lib/external/admin-lte/plugins/overlayScrollbars/css/OverlayScrollbars.min.css">
            <link rel="stylesheet" href="/lib/external/admin-lte/plugins/daterangepicker/daterangepicker.css">
        <title>Chartmuseum</title>
    </head>
    <body class="hold-transition sidebar-mini layout-fixed">
        <div class="wrapper">
            <!-- Navbar -->
            <nav class="main-header navbar navbar-expand navbar-white navbar-light">
                <!-- Left navbar links -->
                <ul class="navbar-nav">
                    <li class="nav-item">
                        <a class="nav-link" data-widget="pushmenu" href="/" role="button"><i class="fas fa-bars"></i></a>
                    </li>
                    <li class="nav-item d-none d-sm-inline-block">
                        <a href="/" class="nav-link">Home</a>
                    </li>
                </ul>
            </nav>
            <!-- /.navbar -->

            <!-- Main Sidebar Container -->
            <aside class="main-sidebar sidebar-dark-primary elevation-4">
                <!-- Brand Logo -->
                <a href="/" class="brand-link">
                    <img src="/images/logo192.png" class="brand-image img-circle elevation-3" style="opacity: .8">
                    <span class="brand-text font-weight-light">Chartmuseum</span>
                </a>
                <!-- Sidebar Menu -->
                <nav class="mt-2">
                    <ul class="nav nav-pills nav-sidebar flex-column" data-widget="treeview" role="menu" data-accordion="false">
                        <li class="nav-item">
                            <a href="/" class="nav-link">
                                <i class="nav-icon fas fa-th"></i>
                                <p>Dashboard</p>
                            </a>
                        </li>
                    </ul>
                </nav>
            </aside>
            <!-- /.sidebar -->

            <!-- Content Wrapper. Contains page content -->
            <div class="content-wrapper">
                <!-- Content Header (Page header) -->
                <div class="content-header">
                    <div class="container-fluid">
                        <div class="row mb-2">
                            <div class="col-sm-6">
                                <h1 class="m-0 text-dark">Helm Chart Details</h1>
                            </div><!-- /.col -->
                            <div class="col-sm-6">
                                <ol class="breadcrumb float-sm-right">
                                    <li class="breadcrumb-item"><a href="/">Home</a></li>
                                    <li class="breadcrumb-item active">Details</li>
                                </ol>
                            </div><!-- /.col -->
                        </div><!-- /.row -->
                    </div><!-- /.container-fluid -->
                </div>
                <!-- /.content-header -->
                <!-- Main content -->
                <section class="content">
                    <div class="container-fluid">
                      <div class="row">
                        <div class="col-12">
                            <div class="card">
                              <!-- /.card-header -->
                              <div class="card-body table-responsive p-0" style="height: 78vh;">
                                <table class="table table-head-fixed text-nowrap">
                                  <thead>
                                    <tr>
                                      <th><input id="chkAll" type="checkbox"></th>
                                      <th>Version</th>
                                      <th>Description</th>
                                      <th>ApiVersion</th>
                                      <th>AppVersion</th>
                                      <th>Created</th>
                                      <th>Actions</th>
                                    </tr>
                                  </thead>
                                  <tbody id="details" name="<%=name%>"></tbody>
                                </table>
                              </div>
                              <!-- /.card-body -->
                            </div>
                            <!-- /.card -->
                          </div>
                      </div>
                      <!-- /.row -->
                      <div class="row">
                          <div class="col-md-6"></div>
                          <div class="col-md-6">
                            <div class="float-sm-right">
                                <button id="btnMultiDelete" type="submit" class="btn btn-info"><i class="fas fa-minus"></i> Multi Delete</a>
                            </div>
                          </div>
                      </div>
                    </div><!-- /.container-fluid -->
                </section>
            </div>
        </div>

        <script defer src="/lib/external/admin-lte/plugins/jquery/jquery.min.js"></script>
        <script defer src="/lib/external/admin-lte/plugins/jquery-ui/jquery-ui.min.js"></script>
        <script defer src="/lib/external/admin-lte/plugins/bootstrap/js/bootstrap.bundle.min.js"></script>
        <script defer src="/lib/external/admin-lte/plugins/moment/moment.min.js"></script>
        <script defer src="/lib/external/admin-lte/plugins/daterangepicker/daterangepicker.js"></script>
        <script defer src="/lib/external/admin-lte/plugins/overlayScrollbars/js/jquery.overlayScrollbars.min.js"></script>
        <script defer src="/lib/external/admin-lte/dist/js/adminlte.js"></script>
        <script defer src="/javascripts/detail/index.js"></script>
    </body>
</html>